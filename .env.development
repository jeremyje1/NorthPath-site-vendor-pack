STRIPE_PRICE_STARTER_FREE=price_test_starter_free
STRIPE_PRICE_TRIAL_FULL_ACCESS=price_test_trial_full_access
STRIPE_PRICE_VENDOR_PACK_STARTER=price_test_vendor_pack_starter
STRIPE_PRICE_VENDOR_PACK_PROFESSIONAL=price_test_vendor_pack_professional
STRIPE_PRICE_VENDOR_PACK_UPDATES_MONTHLY=price_test_updates_monthly
STRIPE_PRICE_VENDOR_PACK_UPDATES_ANNUAL=price_test_updates_annual
STRIPE_PRICE_CONNECTORS_SINGLE_MONTHLY=price_test_connectors_single
STRIPE_PRICE_CONNECTORS_ALL_MONTHLY=price_test_connectors_all_monthly
STRIPE_PRICE_CONNECTORS_ALL_ANNUAL=price_test_connectors_all_annual
STRIPE_PRICE_EVALOPS_MONTHLY=price_test_evalops_monthly
STRIPE_PRICE_EVALOPS_ANNUAL=price_test_evalops_annual
STRIPE_PRICE_BUNDLE_PROFESSIONAL_MONTHLY=price_test_bundle_pro_monthly
STRIPE_PRICE_BUNDLE_PROFESSIONAL_ANNUAL=price_test_bundle_pro_annual
STRIPE_PRICE_BUNDLE_ENTERPRISE_MONTHLY=price_test_bundle_ent_monthly
STRIPE_PRICE_BUNDLE_ENTERPRISE_ANNUAL=price_test_bundle_ent_annual
STRIPE_PRICE_ENTERPRISE_MONTHLY=price_test_enterprise_monthly
STRIPE_PRICE_ENTERPRISE_ANNUAL=price_test_enterprise_annual

# Checkout URLs
NEXT_PUBLIC_CHECKOUT_SUCCESS_URL=http://localhost:3000/success
NEXT_PUBLIC_CHECKOUT_CANCEL_URL=http://localhost:3000/cancel

# Instructions:
# 1. Copy this to .env.local to test the new pricing UI
# 2. Run: npm run dev
# 3. Visit http://localhost:3000/pricing to see the new layout
# 4. All buttons will show "Payment Setup In Progress" since these are test IDs
# 5. Once ready, create real Stripe products and replace the price IDs
